if [ "$OMETAJS_HOME" == "" ]
then
	echo Need the location of OmetaJS to continue.
	echo Install OmetaJS and set \$OMETAJS_HOME to its parent dir \(excluding the /ometa-js/ part\) before running again
	exit -1
fi
java -cp js.jar org.mozilla.javascript.tools.shell.Main ometa-rhino.js "/Users/vinodkd/installed/ometa-js/" bs.ometa bsc.ometa $1
